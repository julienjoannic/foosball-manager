{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run foosball-manager-dev",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch",
        "--in-memory?",
        "--profile",
        "hybrid"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env1",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/foosball-manager\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}